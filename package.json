        "axios": "^0.21.1",
        "electron-edge-js": "^12.18.5",
        "electron-is-dev": "^1.2.0",
        "eslint-plugin-react-hooks": "^4.1.0-rc.0",
        "face-api.js": "^0.9.1",
        "form-data": "^4.0.0",
        "full-powershell": "1.4.0",
        "jquery": "^3.5.1",
