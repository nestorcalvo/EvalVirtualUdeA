        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^5.2.0",
        "cross-env": "^7.0.2",
        "electron": "^12.0.8",
        "electron-builder": "^22.7.0",
